{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "font-awesome@6.2.0",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "open-iconic@1.1.1",
      "destination": "wwwroot/lib/open-iconic/",
      "files": [
        "font/css/open-iconic.min.css",
        "font/css/open-iconic-bootstrap.css",
        "font/css/open-iconic-bootstrap.min.css",
        "font/css/open-iconic-foundation.css",
        "font/css/open-iconic-foundation.min.css",
        "font/css/open-iconic.css",
        "font/fonts/open-iconic.eot",
        "font/fonts/open-iconic.otf",
        "font/fonts/open-iconic.svg",
        "font/fonts/open-iconic.ttf",
        "font/fonts/open-iconic.woff"
      ]
    },
    {
      "library": "bootstrap@5.2.3",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "jquery-validate@1.19.3",
      "destination": "wwwroot/lib/jquery-validate/",
      "files": [
        "jquery.validate.min.js",
        "localization/messages_ru.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "library": "bootstrap-icons@1.10.2",
      "destination": "wwwroot/lib/bootstrap-icons/"
    }
  ]
}